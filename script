    Finished = false, 
    -- T√≠nh nƒÉng ·∫©n key: Khi nh·∫≠p s·∫Ω kh√¥ng hi·ªán ch·ªØ th·∫≠t
    Callback = function(Value) 
        _G.InputOpKey = Value 
    end
})

Tabs.Op:AddButton({
    Title = "üîì X√ÅC NH·∫¨N M·ªû KH√ìA",
    Description = "B·∫•m ƒë·ªÉ ki·ªÉm tra m·∫≠t m√£ v√† hi·ªán Emote",
    Callback = function()
        if _G.InputOpKey == OpKey then
            if not KeyUnlocked then
                KeyUnlocked = true
                Fluent:Notify({Title = "TH√ÄNH C√îNG", Content = "M·∫≠t m√£ ch√≠nh x√°c! ƒê√£ m·ªü kh√≥a Emote.", Duration = 5})
                
                -- CH·ªà HI·ªÜN EMOTE, KH√îNG HI·ªÜN AUTO FARM/AIMBOT
                Tabs.Op:AddSection("üé≠ DANH S√ÅCH EMOTE")
