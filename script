                    Callback = function()
                        loadstring(game:HttpGet("https://raw.githubusercontent.com/7yd7/Hub/refs/heads/Branch/GUIS/Emotes.lua"))()
                    end
                })
            end
        else
            Fluent:Notify({Title = "SAI M·∫¨T M√É", Content = "Vui l√≤ng nh·∫≠p l·∫°i ƒë√∫ng Key c·ªßa Admin!", Duration = 5})
        end
    end
})

